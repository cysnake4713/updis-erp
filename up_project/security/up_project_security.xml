<?xml version="1.0"?>
<openerp>
	<data>
		<record id="group_up_project_user" model="res.groups">
			<field name="name">任意人员</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
		</record>
		<record id="group_up_project_zongshishi" model="res.groups">
			<field name="name">总师室人员</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
        	<field name="implied_ids" eval="[(4, ref('group_up_project_user'))]"/>
		</record>
		<record id="group_up_project_jingyingshi" model="res.groups">
			<field name="name">经营室人员</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
        	<field name="implied_ids" eval="[(4, ref('group_up_project_user'))]"/>
		</record>
		<record id="group_up_project_suozhang" model="res.groups">
			<field name="name">所长</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
        	<field name="implied_ids" eval="[(4, ref('group_up_project_user'))]"/>
		</record>
		<record id="group_up_project_manager" model="res.groups">
			<field name="name">项目负责人</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
        	<field name="implied_ids" eval="[(4, ref('group_up_project_user'))]"/>
		</record>
		<record id="group_up_project_admin" model="res.groups">
			<field name="name">管理员</field>
			<field name="category_id" ref="base.module_category_project_management"></field>
        	<field name="implied_ids" eval="[(4, ref('group_up_project_user'))]"/>
		</record>
		<record id="rule_delete_project" model="ir.rule">
			<field name="name">Only admin can delete project</field>
			<field name="model_id" ref="model_up_project_project"/>
			<field name="domain_force">[('fuzeren_id','=',user.id)]</field>
			<field name="perm_unlink" eval="True" />
			<field name="perm_read" eval="False" />
			<field name="perm_write" eval="False" />
			<field name="perm_create" eval="False" />
		</record>
	</data>
</openerp>