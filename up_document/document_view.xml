<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!--document.directory.access-->
        <record id="view_document_directory_access_tree" model="ir.ui.view">
            <field name="name">document.directory.access.tree</field>
            <field name="model">document.directory.access</field>
            <field name="arch" type="xml">
                <tree string="Access" version="7.0" editable="bottom">
                    <field name="group_id"/>
                    <field name="perm_read"/>
                    <field name="perm_write"/>
                    <field name="perm_create"/>
                    <field name="perm_unlink"/>
                </tree>
            </field>
        </record>
        <!--ir.attachment public tree-->
        <record model="ir.ui.view" id="view_document_file_public_tree">
            <field name="name">ir.attachment.public.tree</field>
            <field name="model">ir.attachment</field>
            <field name="priority" eval="10"/>
            <field name="arch" type="xml">
                <tree colors="blue:type in ('url',)" string="Attachments" create="false" delete="false" edit="false">
                    <field name="datas" filename="datas_fname"/>
                    <field name="datas_fname" invisible="1"/>
                    <field name="parent_id" invisible="1"/>
                    <field name="user_id" invisible="1"/>
                    <field name="write_date"/>
                    <field name="type" invisible="1"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>