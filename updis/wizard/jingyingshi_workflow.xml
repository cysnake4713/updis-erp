<?xml version="1.0"?>
<openerp>
	<data>
		<record model="workflow" id="wkf_project_review_jingyingshishenpi_form">
			<field name="name">项目启动经营室审批流程</field>
			<field name="osv">project.review.jingyingshishenpi.form</field>
			<field name="on_create">True</field>
		</record>
		<!-- Activities -->
		<record id="act_jingyingshishenpi_draft" model="workflow.activity">
			<field name="wkf_id" ref="wkf_project_review_jingyingshishenpi_form" />
			<field name="name">draft</field>
			<field name="flow_start">True</field>
			<field name="kind">function</field>
			<field name="action">update_project_jingyingshishenpi_form()</field>
		</record>
		<record id="act_jingyingshishenpi_submitted" model="workflow.activity">
			<field name="wkf_id" ref="wkf_project_review_jingyingshishenpi_form" />
			<field name="name">Submitted</field>
			<field name="action_id" ref="jingyingshishenpi_multi_submitted"/>
			<field name="kind">function</field>
			<field name="action">write({'state':'submitted'})</field>
		</record>
		<record id="act_jingyingshishenpi_accepted" model="workflow.activity">
			<field name="wkf_id" ref="wkf_project_review_jingyingshishenpi_form" />
			<field name="name">Accepted</field>
			<field name="flow_stop">True</field>
			<field name="kind">function</field>
			<field name="action_id" ref="jingyingshishenpi_multi_accepted"/>
			<field name="action">write({'state':'accepted'})</field>
		</record>
		<record id="act_jingyingshishenpi_rejected" model="workflow.activity">
			<field name="wkf_id" ref="wkf_project_review_jingyingshishenpi_form" />
			<field name="name">Rejected</field>
			<field name="flow_stop">True</field>
			<field name="kind">function</field>
			<field name="action">write({'state':'rejected'})</field>
		</record>
		<!-- Transitions -->
		<record id="trans_jingyingshishenpi_1" model="workflow.transition">
			<field name="act_from" ref="act_jingyingshishenpi_draft"/>
			<field name="act_to" ref="act_jingyingshishenpi_submitted"/>
			<field name="signal">jingyingshishenpi_review_submit</field>
		</record>
		<record id="trans_jingyingshishenpi_2" model="workflow.transition">
			<field name="act_from" ref="act_jingyingshishenpi_submitted"/>
			<field name="act_to" ref="act_jingyingshishenpi_accepted"/>
			<field name="signal">jingyingshishenpi_review_accept</field>
		</record>
		<record id="trans_jingyingshishenpi_3" model="workflow.transition">
			<field name="act_from" ref="act_jingyingshishenpi_submitted"/>
			<field name="act_to" ref="act_jingyingshishenpi_rejected"/>
			<field name="signal">jingyingshishenpi_review_reject</field>
		</record>

	</data>
</openerp>